pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi World'
            }
        }
        stage('hook test') {
                    steps {
                        echo 'testing hook'
                    }
                }
        stage('Hallo') {
            steps {
                echo 'Hallo World'
            }
        }
    }
}